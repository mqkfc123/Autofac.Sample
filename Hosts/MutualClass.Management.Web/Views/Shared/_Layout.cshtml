
@using Sample.Management.Web.Utility
@{
    //Layout = null; 

    var JsVersion = ViewBag.JsVersion;
    var CssVersion = ViewBag.CssVersion;

    var resourceUrl = SystemConfig.ResourceUri;
}
<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="keywords" content="" />
    <meta name="description" content="" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>安社管理中心</title>
    <link rel="Shortcut Icon" href="@resourceUrl/image/admin/icon.png" type="image/x-icon">
    <link href="@resourceUrl/css/global.css?@CssVersion" rel="stylesheet" type="text/css">
    <link href="@resourceUrl/widget/artDialog/dialog.css?@CssVersion" rel="stylesheet" type="text/css">
    <link href="@resourceUrl/css/admin/style.css?@CssVersion" rel="stylesheet" type="text/css"> 
    <link href="@resourceUrl/widget/laydate/yahui/laydate.css?@CssVersion" rel="stylesheet" type="text/css">
    <link href="@resourceUrl/widget/laydate/laydate.css?@CssVersion" rel="stylesheet" type="text/css">
    <script type="text/javascript" src="@resourceUrl/js/jquery-1.11.1.min.js?@JsVersion"></script>
    <script type="text/javascript" src="@resourceUrl/js/admin/main.js?@JsVersion"></script>
    <script type="text/javascript" src="@resourceUrl/widget/artDialog/6.0.5/js/dialog-plus.min.js?@JsVersion"></script>
    <script type="text/javascript" src="@resourceUrl/js/jquery.form.min.js?@JsVersion"></script> 
    <script type="text/javascript" src="@resourceUrl/js/base.js?@JsVersion"></script>
    <script type="text/javascript" src="@resourceUrl/widget/laydate/laydate.js?@JsVersion"></script>
    <script type="text/javascript" src="@resourceUrl/js/dialog.js?@JsVersion"></script>

