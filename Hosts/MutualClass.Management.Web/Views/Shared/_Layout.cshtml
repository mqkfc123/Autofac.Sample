<body class="account_bg">
    <div class="index clearfix">
        <!--侧边栏-->
        <div class="zxsj_left_menu">
            @Html.Partial(NormalizePath("./Menu.cshtml"))
        </div>
        <!--/侧边栏-->
        <!--内容-->
        <div class="index_right">
            <!--公共头部-->
            @*@Html.Partial(NormalizePath("~/Views/Shapes/Footer.cshtml"), data)*@
            @*@Html.RenderPartial("Shapes/AppBarStyle", data);*@
            @Html.Partial(NormalizePath("./User.cshtml"))
            <!--/公共头部-->
            <!--工作台-->
            <div>
                @RenderBody()
            </div>
            <!--/工作台-->
        </div>
        <!--/内容-->
    </div>
</body>
</html>
<script type="text/javascript">
